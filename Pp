--// Ù†Ø¸Ø§Ù… Ø§Ù„Ø³Ø­Ø¨ (Drag)
local d = {a = false}
Main.InputBegan:Connect(function(i) if i.UserInputType == Enum.UserInputType.MouseButton1 then d.a = true d.s = i.Position d.p = Main.Position end end)
UIS.InputChanged:Connect(function(i) if d.a and i.UserInputType == Enum.UserInputType.MouseMovement then local delta = i.Position - d.s Main.Position = UDim2.new(d.p.X.Scale, d.p.X.Offset + delta.X, d.p.Y.Scale, d.p.Y.Offset + delta.Y) end end)
UIS.InputEnded:Connect(function(i) if i.UserInputType == Enum.UserInputType.MouseButton1 then d.a = false end end)

