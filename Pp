--// Ù…Ø­Ø±Ùƒ Ø§Ù„Ø§Ø®ØªÙ„Ø§Ù„ Ø§Ù„Ø¹Ù‚Ù„ÙŠ (The Madness Engine)
local function atomize(obj)
    if not (obj:IsA("RemoteEvent") or obj:IsA("RemoteFunction")) then return end
    if all_objects[obj] then return end
    
    all_objects[obj] = {
        FullName = obj:GetFullName(),
        Type = obj.ClassName,
        Parent = tostring(obj.Parent),
        Time = os.clock()
    }

    local row = Instance.new("Frame")
    row.Size = UDim2.new(1, 0, 0, 40)
    row.BackgroundColor3 = Color3.fromRGB(10, 0, 10)
    row.Parent = Scroll
    
    local txt = Instance.new("TextLabel")
    txt.Size = UDim2.new(1, -10, 1, 0)
    txt.Position = UDim2.new(0, 10, 0, 0)
    txt.Text = ">> [DETECTED] " .. obj.Name .. " | " .. obj.ClassName
    txt.TextColor3 = Color3.fromRGB(255, 0, 255)
    txt.BackgroundTransparency = 1
    txt.TextXAlignment = Enum.TextXAlignment.Left
    txt.Font = Enum.Font.Code
    txt.TextSize = 11
    txt.Parent = row
    
    Scroll.CanvasSize = UDim2.new(0, 0, 0, Scroll.UIListLayout.AbsoluteContentSize.Y)
end

